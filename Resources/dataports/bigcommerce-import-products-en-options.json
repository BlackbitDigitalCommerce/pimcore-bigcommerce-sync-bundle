            "calculation": "// Enter id of dataport to be executed\n$dataportId = {{ DEPENDENT DATAPORT ID }};\n\n// provide parameters to use in dynamic import resource or in callback functions of dependent dataport\n// leave empty to run dependent dataport with default import source\n$parameters = {{ DEPENDENT DATAPORT PARAMETERS }};\n\n\nif(($parameters || $params['lastCall']) && $dataportId) {\n    setRelations($params);\n    \n    $command = 'dd:complete '.$dataportId.' --rm'.($parameters?' -f --parameters=\"'.str_replace(['\"', '$', '`'], ['\\\"', '\\$', '\\`'], json_encode($parameters)).'\"':'');\n    $params['logger']->info($command);\n    if({{ DEPENDENT DATAPORT WAIT UNTIL FINISHED }}) {\n        $params['logger']->info('Executing command \"'.$command.'\"');\n        $output = \\Blackbit\\DataDirectorBundle\\lib\\Pim\\Cli::exec($command);\n        \n        if(preg_match('/X-Data-Director-Run:\\s*(.+)(\\n|$)/', $output, $dependentRunId)) {\n            $logFileObjectPath = $dataportId.'/'.trim($dependentRunId[1]);\n            $logStorage = \\Blackbit\\DataDirectorBundle\\lib\\Pim\\Helper::getApplicationLogStorage();\n            if($logStorage->fileExists($logFileObjectPath)) {\n                $dependentDataportLogs = $logStorage->read($logFileObjectPath);\n                $dependentDataportLogLines = explode(\"\\n\", $dependentDataportLogs);\n                $dependentDataportLogLines = array_map(function ($line) {\n                    return str_repeat(' ', 2).$line;\n                }, $dependentDataportLogLines);\n    \n                $params['logger']->info(\"Logs from dependent dataport run: \\n\".implode(\"\\n\", $dependentDataportLogLines));\n            }\n                \n            $responseDocument = \\Blackbit\\DataDirectorBundle\\Tools\\Installer::getCachePath().'/result_'.$dataportId.'_'.trim($dependentRunId[1]);\n            \n            if(file_exists($responseDocument)) {\n                return \\unserialize(\\file_get_contents($responseDocument));\n            }\n        }\n    } else {\n        $params['logger']->info('Queuing command \"'.$command.'\"');\n        $queue = new \\Blackbit\\DataDirectorBundle\\model\\Queue();\n        $queue->create(['command' => $command, 'triggered_by' => 'Result callback function of dataport '.$params['context']['dataportId']]);\n    }\n}\n\nfunction setRelations($params) {\n    $product = \\Pimcore\\Model\\DataObject\\Product::getByBigcommerce_id($params['rawItemData']['bigcommerce_product_id']['value'], 'en', 1);\n    $options = \\Pimcore\\Model\\DataObject\\Option::getByBigcommerce_product_id($params['rawItemData']['bigcommerce_product_id']['value'], 'en');\n    $product->setOptions($options->load(), 'en');\n    \n    $product->save();\n}",
